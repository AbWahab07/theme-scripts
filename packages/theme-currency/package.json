{
  "name": "@shopify/theme-currency",
  "version": "1.0.0-alpha.1",
  "description":
    "A library that helps with managing currencies in Shopify Themes",
  "main": "currency.cjs.js",
  "modules": "currency.es5.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shopify/theme-scripts.git"
  },
  "keywords": ["slate"],
  "author": "Shopify Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shopify/theme-scripts/issues"
  },
  "homepage": "https://github.com/Shopify/theme-scripts#readme",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run build:es5 & npm run build:cjs",
    "build:es5":
      "../../node_modules/.bin/babel src/currency.js --out-file currency.es5.js",
    "build:cjs":
      "../../node_modules/.bin/babel --no-babelrc src/currency.js --out-file currency.cjs.js --presets=shopify"
  }
}
